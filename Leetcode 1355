select distinct f.activity
from friends f
join 
(select count(*) as cnt, activity from friends group by activity) a
on f.activity = a.activity
where a.cnt not in 
(select max(cnt) as cnt
from
(select count(*) as cnt, activity
from friends
group by activity) b
union all
 select min(cnt) as cnt
from
(select count(*) as cnt, activity
from friends
group by activity) b
)
